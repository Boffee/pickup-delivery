<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	
	<agent name="reactive-0">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveFinal"/>
		<set discount-factor="0"/>
	</agent>
	
	<agent name="reactive-.5">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveFinal"/>
		<set discount-factor=".5"/>
	</agent>
	
	<agent name="reactive-.85">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveFinal"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive-1">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveFinal"/>
		<set discount-factor="1"/>
	</agent>
	
	<agent name="reactive-1perKm">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRewardPerKm"/>
		<set discount-factor="1"/>
	</agent>
	
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRandom"/>
		<set discount-factor=".85"/>
	</agent>
	
</agents>
